// Description: A trigger for creating new Contacts. It's before_insert only.

trigger contactTrigger on Contact (before insert) {
    switch on trigger.operationtype {
        when BEFORE_INSERT {
            contactTriggerHandler.siteRelater(trigger.new);
        }
    }
}